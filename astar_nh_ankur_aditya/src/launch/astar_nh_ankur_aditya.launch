<launch>
    <!-- Load the Gazebo simulator with the specified world file -->
    <include file="$(find gazebo_ros)/launch/empty_world.launch">
        <arg name="world_name" value="$(find my_turtlebot3_nav)/worlds/map.world"/>
    </include>
  
    <!-- Spawn the Turtlebot3 robot model in the Gazebo simulator -->
    <node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model" args="-urdf -param /robot_description -model turtlebot3"/>
  
    <!-- Launch the goal_publisher node to wait for user input -->
    <node name="goal_publisher" pkg="my_turtlebot3_nav" type="goal_publisher.py" output="screen"/>
  
    <!-- Launch the path_publisher node to compute and publish the path -->
    <node name="path_publisher" pkg="my_turtlebot3_nav" type="path_publisher.py" output="screen"/>
  
  </launch>